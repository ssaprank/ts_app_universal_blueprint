{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es6",
    "noEmit": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "allowJs": true
  },
  "include": [ "./src/**/*.tsx", "./src/**/*.ts" ],
}
